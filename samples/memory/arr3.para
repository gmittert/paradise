module main
I64 main() {
    Char[] word = "abcd";
    Char[] word2 = "efgh";

    word[0] = 'e';
    word[1] = 'f';
    word[2] = 'g';
    word[3] = 'h';
    U64 x = 0;
    I64 ret = 3;
    while (x < #word) {
        if (word[x] != word2[x]) {
            ret = 1;
        }
        x = x + 1;
    }
    return ret;
}
